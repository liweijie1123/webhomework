<div class="m-logo">
    <img src="img/logo.png" alt="">
    <span class="u-l-title">{{:longtitle}}</span>
    <span  class="u-s-title">{{:shorttitle}}</span>
</div>
<div class="m-sp"></div>
<div class="m-nav">
    {{for navs}}
    <li class="nav-item">
        <a href="#">{{:name}}</a>
        {{if lists}}
        <ul class="items">
            {{for lists}}
            <li><a href="#">{{: #data}}</a></li>
            {{/for}}
        </ul>
        {{/if}}
    </li>
    {{/for}}
</div>